    addOrUpdateDeliveryRequest:
      required:
        - externalId
        - time
        - progress
        - status
        - truckId
      type: object
      properties:
        externalId:
          minLength: 1
          type: string
        time:
          minLength: 1
          type: string
          format: date-time
        progress:
          minLength: 1
          type: string
        status:
          minLength: 1
          type: string
        truckId:
          minLength: 1
          type: string
    addOrUpdateGuestAccessRequest:
      required:
        - deliveryId
        - guestId
      type: object
      properties:
